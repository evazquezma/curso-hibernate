<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"  updateCheck="false"
	monitoring="autodetect" dynamicConfig="true" name="xumco">

	<diskStore path="${java.io.tmpdir}/PRUEBAS"/>


	<defaultCache maxElementsInMemory="20000" 
		eternal="false"
		timeToIdleSeconds="300" 
		timeToLiveSeconds="300" 
		overflowToDisk="true"/>



	<cache name="es.pruebas.hibernate.model.Factura"  
        maxElementsInMemory="850"  
        eternal="false"  
        timeToIdleSeconds="300"  
        timeToLiveSeconds="300"  
        overflowToDisk="true"/>    

</ehcache>

